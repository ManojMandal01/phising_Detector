{
  "version": 2,
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python",
      "config": {
        "includeFiles": [
          "data_bal - 20000.xlsx"
        ]
      }
    }
  ],
  "routes": [
    { "src": "/check_url", "dest": "server.py" }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}
